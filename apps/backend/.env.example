# Backend Environment Variables
# Copy to .env and fill in values

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/dar

# JWT (minimum 32 characters)
JWT_SECRET=dev-secret-change-me-in-production-minimum-32-chars

# Service Token (for Moltbot tool calls, minimum 16 characters)
SERVICE_TOKEN=dev-service-token-change-me

# Moltbot Gateway (server-side only)
MOLTBOT_GATEWAY_URL=ws://127.0.0.1:18789
MOLTBOT_GATEWAY_TOKEN=change-me-in-production

# Notification Providers (optional)
WHATSAPP_API_TOKEN=
TELEGRAM_BOT_TOKEN=

# Environment
NODE_ENV=development
PORT=3001
